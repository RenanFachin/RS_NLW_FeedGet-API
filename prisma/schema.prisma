generator client {
  provider = "prisma-client-js"
}

generator erd {
  provider = "prisma-erd-generator"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Feedback {
  id         String  @id @default(uuid())

  type       String
  comment    String
  screenshot String?

  @@map("feedbacks")
}
